package com.hongfox.domain;

public class NoteBook implements Equitment{
	private String model;
	private double price;
	
	
	
	public NoteBook() {
		super();
	}



	public NoteBook(String model, double price) {
		super();
		this.model = model;
		this.price = price;
	}



	@Override
	public String getDescription() {
		return model + "(" + price + ")";
	}
	
}
